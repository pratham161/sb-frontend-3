---

---

<section id="contact" class="relative text-white py-16 px-6 overflow-hidden">
	<!-- Google Map Background -->
	<div class="absolute inset-0 z-0">
		<!-- <iframe 
			src="https://maps.app.goo.gl/85BGJ1KfkqTLrhjr6" 
			width="100%" 
			height="100%" 
			style="border:0;position:absolute;top:0;left:0;" 
			allowfullscreen="" 
			loading="lazy" 
			referrerpolicy="no-referrer-when-downgrade">
		</iframe> -->
		<iframe
			src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3792.136088317382!2d74.61651507582195!3d18.11150568154056!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bc39f3fc0ab08a5%3A0xe3d93ccb18c68987!2sZargadwadi%20Rd%2C%20Maharashtra%20413102!5e0!3m2!1sen!2sin!4v1770650785906!5m2!1sen!2sin"
			width="100%" 
			height="100%" 
			style="border:0;position:absolute;top:0;left:0;" 
			allowfullscreen="" 
			loading="lazy"
			referrerpolicy="no-referrer-when-downgrade"></iframe>
		<!-- Overlay for better text visibility -->
		<div class="absolute inset-0 bg-black/60"></div>
	</div>

	<!-- Content -->
	<div class="max-w-4xl mx-auto relative z-10">
		<!-- Section Header -->
		<div class="text-center mb-12">
			<h2 class="text-3xl md:text-4xl font-extrabold mb-4 text-red-500">
				Get In Touch
			</h2>
			<p class="text-lg text-white max-w-2xl mx-auto mb-2">
				Have questions or ready to transform your health? We'd love to
				hear from you!
			</p>
			<div class="w-16 h-1 bg-green-500 rounded mx-auto"></div>
		</div>

		<div class="grid md:grid-cols-2 gap-8">
			<!-- Contact Information -->
			<div class="space-y-8">
				<div
					class="bg-gray-800/50 backdrop-blur-sm border border-green-500/30 rounded-lg p-6 hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<div class="flex items-start space-x-4">
						<div class="flex-shrink-0">
							<div
								class="flex items-center justify-center h-12 w-12 rounded-lg bg-green-500/20 border border-green-500/50"
							>
								<svg
									class="h-6 w-6 text-green-400"
									fill="none"
									stroke="currentColor"
									viewBox="0 0 24 24"
								>
									<path
										stroke-linecap="round"
										stroke-linejoin="round"
										stroke-width="2"
										d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z"
									></path>
								</svg>
							</div>
						</div>
						<div>
							<h3 class="text-lg font-bold text-yellow-400 mb-1">
								Email
							</h3>
							<p class="text-white">
								symbicroftbiotech@gmail.com
							</p>
							<p class="text-sm text-gray-400">
								We'll respond within 24 hours
							</p>
						</div>
					</div>
				</div>

				<div
					class="bg-gray-800/50 backdrop-blur-sm border border-green-500/30 rounded-lg p-6 hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<div class="flex items-start space-x-4">
						<div class="flex-shrink-0">
							<div
								class="flex items-center justify-center h-12 w-12 rounded-lg bg-green-500/20 border border-green-500/50"
							>
								<svg
									class="h-6 w-6 text-green-400"
									fill="none"
									stroke="currentColor"
									viewBox="0 0 24 24"
								>
									<path
										stroke-linecap="round"
										stroke-linejoin="round"
										stroke-width="2"
										d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"
									></path>
								</svg>
							</div>
						</div>
						<div>
							<h3 class="text-lg font-bold text-yellow-400 mb-1">
								Phone
							</h3>
							<p class="text-white">+91 90756 88283</p>
							<p class="text-sm text-gray-400">
								Available Mon-Fri, 9AM-6PM IST
							</p>
						</div>
					</div>
				</div>

				<div
					class="bg-gray-800/50 backdrop-blur-sm border border-green-500/30 rounded-lg p-6 hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<div class="flex items-start space-x-4">
						<div class="flex-shrink-0">
							<div
								class="flex items-center justify-center h-12 w-12 rounded-lg bg-green-500/20 border border-green-500/50"
							>
								<svg
									class="h-6 w-6 text-green-400"
									fill="none"
									stroke="currentColor"
									viewBox="0 0 24 24"
								>
									<path
										stroke-linecap="round"
										stroke-linejoin="round"
										stroke-width="2"
										d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"
									></path>
									<path
										stroke-linecap="round"
										stroke-linejoin="round"
										stroke-width="2"
										d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"
									></path>
								</svg>
							</div>
						</div>
						<div>
							<h3 class="text-lg font-bold text-yellow-400 mb-1">
								Location
							</h3>
							<p class="text-white">
								M.No.364, Bramhananda Bungalow, Sutarnet, Kasaba
							</p>
							<p class="text-sm text-gray-400">Baramati 413102</p>
						</div>
					</div>
				</div>
			</div>

			<!-- Contact Form -->
			<div
				class="bg-gray-800/50 backdrop-blur-sm border border-green-500/30 rounded-lg p-8 hover:border-green-400/60 transition-all duration-200"
			>
				<form class="space-y-6">
					<!-- Name Field -->
					<div>
						<label
							for="name"
							class="block text-sm font-semibold text-white mb-2"
							>Full Name</label
						>
						<input
							type="text"
							id="name"
							name="name"
							placeholder="Your name"
							class="w-full px-4 py-3 bg-gray-700/50 border border-green-500/30 rounded-lg text-white placeholder-gray-500 focus:outline-none focus:border-green-400 focus:ring-1 focus:ring-green-400 transition-all duration-200"
							required
						/>
					</div>

					<!-- Email Field -->
					<div>
						<label
							for="email"
							class="block text-sm font-semibold text-white mb-2"
							>Email Address</label
						>
						<input
							type="email"
							id="email"
							name="email"
							placeholder="your@email.com"
							class="w-full px-4 py-3 bg-gray-700/50 border border-green-500/30 rounded-lg text-white placeholder-gray-500 focus:outline-none focus:border-green-400 focus:ring-1 focus:ring-green-400 transition-all duration-200"
							required
						/>
					</div>

					<!-- Subject Field -->
					<div>
						<label
							for="subject"
							class="block text-sm font-semibold text-white mb-2"
							>Subject</label
						>
						<input
							type="text"
							id="subject"
							name="subject"
							placeholder="How can we help?"
							class="w-full px-4 py-3 bg-gray-700/50 border border-green-500/30 rounded-lg text-white placeholder-gray-500 focus:outline-none focus:border-green-400 focus:ring-1 focus:ring-green-400 transition-all duration-200"
							required
						/>
					</div>

					<!-- Message Field -->
					<div>
						<label
							for="message"
							class="block text-sm font-semibold text-white mb-2"
							>Message</label
						>
						<textarea
							id="message"
							name="message"
							rows="5"
							placeholder="Tell us about your inquiry..."
							class="w-full px-4 py-3 bg-gray-700/50 border border-green-500/30 rounded-lg text-white placeholder-gray-500 focus:outline-none focus:border-green-400 focus:ring-1 focus:ring-green-400 transition-all duration-200 resize-none"
							required></textarea>
					</div>

					<!-- Submit Button -->
					<button
						type="submit"
						class="w-full px-6 py-3 bg-gradient-to-r from-green-500 to-green-600 hover:from-green-600 hover:to-green-700 text-black font-bold rounded-lg transition-all duration-200 shadow-lg hover:shadow-xl hover:shadow-green-500/30 active:scale-95"
					>
						Send Message
					</button>

					<!-- Privacy Notice -->
					<p class="text-xs text-gray-400 text-center">
						We respect your privacy. Your information will be kept
						confidential and used only to respond to your inquiry.
					</p>
				</form>
			</div>
		</div>

		<!-- Additional CTA -->
		<div class="mt-12 text-center">
			<p class="text-white mb-6">
				Have a question? Check out our <a
					href="#"
					class="text-green-400 hover:text-green-300 font-semibold transition-colors"
					>FAQ page</a>
			</p>
			<div class="flex gap-4 justify-center">
				<a
					href="#"
					class="inline-flex items-center space-x-2 px-6 py-3 bg-gray-800/50 border border-green-500/30 rounded-lg text-white hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<svg
						class="w-5 h-5"
						fill="currentColor"
						viewBox="0 0 24 24"
					>
						<path
							d="M8.29 20v-7.21H5.33V9.25h2.96V7.02c0-2.92 1.78-4.51 4.39-4.51 1.25 0 2.33.09 2.63.13v3.05h-1.81c-1.41 0-1.69.67-1.69 1.66V9.25h3.37l-.44 3.54h-2.93V20"
						></path>
					</svg>
					<span>Facebook</span>
				</a>
				<a
					href="#"
					class="inline-flex items-center space-x-2 px-6 py-3 bg-gray-800/50 border border-green-500/30 rounded-lg text-white hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<svg
						class="w-5 h-5"
						fill="currentColor"
						viewBox="0 0 24 24"
					>
						<path
							d="M12 2.04C6.5 2.04 2 6.53 2 12.09C2 17.62 6.5 22 12 22c5.5 0 10-4.38 10-9.91C22 6.53 17.5 2.04 12 2.04M12 20c-4.41 0-8-3.59-8-8s3.59-8 8-8 8 3.59 8 8-3.59 8-8 8m3.5-9c.83 0 1.5-.67 1.5-1.5S16.33 8 15.5 8 14 8.67 14 9.5s.67 1.5 1.5 1.5m-7 0c.83 0 1.5-.67 1.5-1.5S9.33 8 8.5 8 7 8.67 7 9.5 7.67 11 8.5 11m3.5 6.5c2.33 0 4.31-1.46 5.11-3.5H6.89c.8 2.04 2.78 3.5 5.11 3.5z"
						></path>
					</svg>
					<span>Instagram</span>
				</a>
				<a
					href="#"
					class="inline-flex items-center space-x-2 px-6 py-3 bg-gray-800/50 border border-green-500/30 rounded-lg text-white hover:border-green-400/60 hover:bg-gray-800/70 transition-all duration-200"
				>
					<svg
						class="w-5 h-5"
						fill="currentColor"
						viewBox="0 0 24 24"
					>
						<path
							d="M22.46 6c-.87.39-1.8.65-2.77.77 1-6-.6 1.98-2.44 3.27-1.16-.65-2.53-1.03-3.96-1.03-3 0-5.44 2.43-5.44 5.44 0 .43.05.84.15 1.24-4.53-.23-8.55-2.4-11.24-5.7-.47.81-.74 1.75-.74 2.75 0 1.89.96 3.56 2.42 4.54-.89-.03-1.73-.27-2.46-.68v.07c0 2.64 1.88 4.84 4.37 5.34-.46.13-.94.2-1.44.2-.35 0-.7-.03-1.04-.1.7 2.19 2.75 3.77 5.16 3.82-1.87 1.47-4.23 2.34-6.79 2.34-.44 0-.88-.03-1.31-.09 2.43 1.56 5.32 2.47 8.43 2.47 10.11 0 15.62-8.37 15.62-15.63 0-.24 0-.48-.01-.71.07-.51 1.07-1.82 2.07-2.82z"
						></path>
					</svg>
					<span>Twitter</span>
				</a>
			</div>
		</div>
	</div>
</section>

<style>
	input:focus,
	textarea:focus {
		background-color: rgba(55, 65, 81, 0.75);
	}
</style>
